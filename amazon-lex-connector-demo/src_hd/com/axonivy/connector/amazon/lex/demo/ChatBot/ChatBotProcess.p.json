          "code" : [
            "import com.axonivy.connector.amazon.lex.demo.Message;",
            "Message message;",
            "message.what = in.input;",
            "message.icon = \"si si-single-neutral-actions\";",
            "out.messages.add(message);"
          ]
        }
      },
      "visual" : {
        "at" : { "x" : 208, "y" : 160 },
        "size" : { "width" : 144 }
